{
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.[tj]sx?$": "ts-jest"
  },
  "moduleNameMapper": {
    "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/src/tests/mocks/fileMock.js",
    "\\.(css|less|sass|scss)$": "identity-obj-proxy"
  },
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [ "node_modules", "tests", "cpp", "victory" ],
  "coverageDirectory": "<rootDir>/coverage/",
  "modulePathIgnorePatterns": [ "victory", "src/tests/unit/ChartTest.test.tsx", "src/tests/unit/BaseComponent.test.tsx" ]
}